{
	"name": "scraping",
	"properties": {
		"description": "Scrape just 10 folders of users",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "OutputDataset",
						"type": "DatasetReference"
					},
					"name": "MailSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "DerivedColumns"
				}
			],
			"script": "source(allowSchemaDrift: true,\n\tvalidateSchema: false) ~> MailSource\nMailSource derive(User_Name = byName(byName(\"fields\"))) ~> DerivedColumns"
		}
	}
}